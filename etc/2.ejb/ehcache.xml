<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="true"
		diskPersistent="false" />
	<cache name="CORECACHE" maxElementsInMemory="1000000" eternal="true"
		overflowToDisk="true">
	</cache>
	<cache name="WEBCACHE" maxElementsInMemory="1000000" eternal="true"
		overflowToDisk="true">
	</cache>
</ehcache>



